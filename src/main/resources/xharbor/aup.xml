<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    
    <bean class="org.jocean.xharbor.util.ApplyUrlencode4Post"
        init-method="start" destroy-method="destroy">
        <constructor-arg value="${path}"/>
        <constructor-arg value="${noapplyFeature}"/>
        <constructor-arg>
            <bean class="org.springframework.util.StringUtils"
                factory-method="commaDelimitedListToStringArray">
                <constructor-arg value="${applyKeys}"/>
            </bean>
        </constructor-arg>
        <constructor-arg ref="compositeBuilder"/>
    </bean>

</beans>
